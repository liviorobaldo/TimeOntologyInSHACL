abox:tb  time:before       _:b0 ;
        time:before        _:b1 ;
        time:before        abox:t ;
        rdf:type           time:Instant ;
        time:hasBeginning  abox:tb ;
        rdf:type           time:plusInfinite ;
        time:equals        abox:tb ;
        rdf:type           time:TemporalEntity ;
        time:hasEnd        abox:tb .

_:b2    rdf:type           time:TemporalEntity ;
        time:hasBeginning  _:b2 ;
        time:equals        _:b2 ;
        rdf:type           time:Instant ;
        time:hasEnd        _:b2 .

abox:T  rdf:type           time:TemporalEntity ;
        time:hasBeginning  abox:tb ;
        time:hasEnd        _:b2 .

_:b0    rdf:type           time:TemporalEntity ;
        time:equals        _:b0 ;
        rdf:type           time:Instant ;
        time:hasBeginning  _:b0 ;
        time:hasEnd        _:b0 .

_:b1    time:equals        _:b1 ;
        rdf:type           time:Instant ;
        rdf:type           time:TemporalEntity ;
        time:hasBeginning  _:b1 ;
        time:hasEnd        _:b1 .

abox:t  time:hasBeginning  _:b1 ;
        rdf:type           time:TemporalEntity ;
        time:hasEnd        _:b0 .
